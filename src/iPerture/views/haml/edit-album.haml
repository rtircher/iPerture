%html
  %head
    %link(href='http://fonts.googleapis.com/css?family=Quando' rel='stylesheet' type='text/css')
    %link(rel="icon" type="image/png" href="/img/favicon.png")
    %link(rel="stylesheet" type="text/css" href="/css/reset.css")
    %link(rel="stylesheet" type="text/css" href="/css/edit-album.css")

    :javascript
      // Configs for google closure framework
      CLOSURE_NO_DEPS=true;
      CLOSURE_BASE_PATH="./public/";

    %script(type="text/javascript" src="/js/lib/jquery-1.7.2.js")
    %script(type="text/javascript" src="/js/lib/underscore.1.3.1.js")
    %script(type="text/javascript" src="/js/main.js")
    %title Edit album:

  %body.background-style
    %h1.album-title My album
    %a.photo(style="background-image:url('/img/1.png')")
    %a.photo(style="background-image:url('/img/2.png')")
    %a.photo(style="background-image:url('/img/3.png')")
    %a.photo(style="background-image:url('/img/4.png')")
    %a.photo(style="background-image:url('/img/5.png')")
    %a.photo(style="background-image:url('/img/6.png')")
    %form.add-photo-form.photo
      %input(type='file' accept='image/*' name='add-photo-button' value='Add Photo(s)')
      .add-photo-button Add photos
