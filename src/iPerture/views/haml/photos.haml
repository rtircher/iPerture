!!! 5

%html
  %head
    %link(href='http://fonts.googleapis.com/css?family=Quando' rel='stylesheet' type='text/css')
    %link(rel="icon" type="image/png" href="/img/favicon.png")
    %link(rel="stylesheet" type="text/css" href="/css/reset.css")
    %link(rel="stylesheet" type="text/css" href="/css/photos.css")

    :javascript
      // Configs for google closure framework
      CLOSURE_NO_DEPS=true;
      CLOSURE_BASE_PATH="./public/";

    %script(type="text/javascript" src="/js/lib/jquery-1.7.2.js")
    %script(type="text/javascript" src="/js/lib/underscore.1.3.1.js")
    %script(type="text/javascript" src="/js/main.js")
    %title My PhotoStream

  %body.background-style
    .slider-wrapper
      .photos-container
        %a.previous.nav-button
        %a.next.nav-button
        .main-photo
          .full.photo(style="background-image:url('/img/1.png')")

      .thumbnails-container
        %table
          %tr.thumbnails-list
            %td.thumbnail-cell
              %a.thumbnail.photo(style="background-image:url('/img/1.png')")
            %td.thumbnail-cell
              %a.selected.thumbnail.photo(style="background-image:url('/img/2.png')")
            %td.thumbnail-cell
              %a.thumbnail.photo(style="background-image:url('/img/3.png')")
            %td.thumbnail-cell
              %a.thumbnail.photo(style="background-image:url('/img/3.png')")
            %td.thumbnail-cell
              %a.thumbnail.photo(style="background-image:url('/img/3.png')")
            %td.thumbnail-cell
              %a.thumbnail.photo(style="background-image:url('/img/3.png')")
